/* pages/login/login.wxss */

.f-tc {
  text-align: center;
}

.f-tc button {
  width: 240rpx;
  height: 56rpx;
}

.logo_img {
  width: 560rpx;
  height: 300rpx;
}

.u-title {
  font-size: 48rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.m-input-group {
  padding: 0 40rpx;
}

.m-input-cell {
  margin-bottom: 40rpx;
}

.m-cell-title {
  margin-bottom: 20rpx;
  font-size: 28rpx;
}

.m-cell-input {
  display: block;
}

.m-cell-input input {
  margin: auto;
  z-index: 2;
  width: 600rpx;
  height: 78rpx;
  border: 1px solid rgb(102, 102, 102);
  padding-left: 4rpx;
  padding-right: 4rpx;
  box-sizing: border-box;
  outline: none;
  line-height: 74rpx;
  text-align: left;
  font-size: 28rpx;
  font-weight: 400;
  font-style: normal;
  text-decoration: none;
  font-family: 微软雅黑;
  color: rgb(0, 0, 0);
  background-color: rgb(255, 255, 255);
  border-radius: 0px;
}
